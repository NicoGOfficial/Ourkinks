    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "migrate": "^1.7.0",
    "mkdirp": "^1.0.3",
    "moment": "^2.24.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.9.1",
    "multer": "^1.4.2",
    "mustache": "^4.0.0",
    "nestjs-config": "^1.4.5",
