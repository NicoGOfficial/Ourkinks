    "qs": "^6.10.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.0",
    "rxjs": "^7.5.7",
    "sharp": "^0.31.1",
    "socket.io": "^4.5.2",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.1",
    "xml2js": "^0.4.23"
  },
