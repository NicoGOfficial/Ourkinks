    "@nestjs/platform-socket.io": "^9.1.4",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/websockets": "^9.1.4",
    "@socket.io/redis-adapter": "^7.2.0",
    "agenda": "^3.1.0",
    "axios": "^0.19.2",
    "bee-queue": "^1.2.3",
